{
  "name": "@mikesposito/tunnelify",
  "version": "0.2.0",
  "description": "A simple tool that exposes static folders from your local machine to the web",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mikesposito/tunnelify.git"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/mikesposito/tunnelify/issues"
  },
  "homepage": "https://github.com/mikesposito/tunnelify#readme",
  "bin": {
    "tunnelify": "./dist/cli.js"
  },
  "keywords": [
    "tunnelify",
    "serve",
    "expose",
    "files"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "serve": "tsc -p tsconfig.build.json --watch",
    "test": "jest --collect-coverage"
  },
  "author": "Michele Esposito<michele@esposito.codes>",
  "license": "MIT",
  "dependencies": {
    "@mikesposito/tunnelify-cli": "^0.2.0",
    "@mikesposito/tunnelify-local-server": "^0.2.0",
    "socket.io-client": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^16.6.1",
    "jest": "^27.0.6",
    "portastic": "^1.0.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
